{
  "name": "rnweb",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "ios": "react-native run-ios",
    "android": "react-native run-android",
    "web-build": "make-dir dist && cp -f web/index.html dist/index.html && cross-env NODE_ENV=development ./node_modules/.bin/webpack-cli --config ./web/webpack.config.js",
    "web-release": "./node_modules/.bin/webpack -p --config ./web/webpack.config.js",
    "web": "yarn web-build && ./node_modules/.bin/webpack-dev-server -d --config ./web/webpack.config.js --progress --inline --hot --colors",
    "test": "jest"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "core-js": "^3.1.2",
    "crypto-js": "^3.1.9-1",
    "lodash": "^4.17.21",
    "native-base": "^2.12.0",
    "prop-types": "latest",
    "querystring": "^0.2.0",
    "react": "^16.8.6",
    "react-art": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-native": "0.59.8",
    "react-native-cached-image": "^1.4.3",
    "react-native-code-push": "^5.6.0",
    "react-native-device-info": "^0.29.0",
    "react-native-fetch-blob": "^0.10.8",
    "react-native-gesture-handler": "^1.1.0",
    "react-native-i18n": "^2.0.15",
    "react-native-image-picker": "^0.28.0",
    "react-native-linear-gradient": "^2.5.3",
