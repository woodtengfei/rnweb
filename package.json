    "react-native-swiper": "^1.5.14",
    "react-native-web": "^0.11.2",
    "react-native-webview": "^5.3.0",
    "react-navigation": "^3.3.2",
    "rnpm": "^1.9.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
